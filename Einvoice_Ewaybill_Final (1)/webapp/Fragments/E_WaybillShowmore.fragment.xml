<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:table="sap.ui.table"
	xmlns:u="sap.ui.unified" xmlns:core="sap.ui.core" id="inputdialog">
	<Dialog showHeader="false">
		<Panel backgroundDesign="Transparent">
			<!--E_WaybillShowmore-->
			<headerToolbar>
				<OverflowToolbar height="3rem" class="Panel_header">
					<Title id="Vbeln" text="Document Details" class="labelselectrep"/>
					<ToolbarSpacer/>
					<Button icon="sap-icon://decline" press="onCancel1"></Button>
				</OverflowToolbar>
			</headerToolbar>
			<table:Table id="table2" selectionMode="None" visibleRowCount="4" visible="false">
				<table:columns>
					<table:Column hAlign="Center" width="5rem" visible="{/Visible}">
						<Label design="Bold" text="Billing Item" wrapping="true"/>
						<table:template>
							<Text text="{Posnr}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem" visible="{/Visible}">
						<Label design="Bold" text="Material" wrapping="true"/>
						<table:template>
							<Text text="{Matnr}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="9rem" visible="{/Visible}">
						<Label design="Bold" text="Material Description" wrapping="true"/>
						<table:template>
							<Text text="{Arktx}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="6rem">
						<Label design="Bold" text="Quantity" wrapping="true"/>
						<table:template>
							<Text text="{Fkimg}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="4rem">
						<Label design="Bold" text="UOM" wrapping="true"/>
						<table:template>
							<Text text="{Vrkme}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem">
						<Label design="Bold" text="Plant" wrapping="true"/>
						<table:template>
							<Text text="{Werks}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="6rem">
						<Label design="Bold" text="HSN Code" wrapping="true"/>
						<table:template>
							<Text text="{Steuc}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem">
						<Label design="Bold" text="Currency" wrapping="true"/>
						<table:template>
							<Text text="{Waerk}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="6rem">
						<Label design="Bold" text="Unit Price" wrapping="true"/>
						<table:template>
							<Text text="{Unitprice}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="7rem">
						<Label design="Bold" text="Other-Taxable Value" wrapping="true"/>
						<table:template>
							<Text text="{OtherTamt}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="6rem">
						<Label design="Bold" text="Gross Amount" wrapping="true"/>
						<table:template>
							<Text text="{GrossAmt}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem">
						<Label design="Bold" text="Discount" wrapping="true"/>
						<table:template>
							<Text text="{Discount}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="6rem">
						<Label design="Bold" text="Taxable Value" wrapping="true"/>
						<table:template>
							<Text text="{TaxableVal}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="4rem">
						<Label design="Bold" text="GST Rate" wrapping="true"/>
						<table:template>
							<Text text="{GstRate}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem">
						<Label design="Bold" text="IGST Value" wrapping="true"/>
						<table:template>
							<Text text="{IgstAmt}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem">
						<Label design="Bold" text="SGST Value" wrapping="true"/>
						<table:template>
							<Text text="{SgstAmt}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="5rem">
						<Label design="Bold" text="CGST Value" wrapping="true"/>
						<table:template>
							<Text text="{CgstAmt}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="7rem">
						<Label design="Bold" text="Other Non-Taxable Value" wrapping="true"/>
						<table:template>
							<Text text="{OtherNamt}"/>
						</table:template>
					</table:Column>
					<table:Column hAlign="Center" width="6rem">
						<Label design="Bold" text="Item Total Value" wrapping="true"/>
						<table:template>
							<Text text="{ItmAmt}"/>
						</table:template>
					</table:Column>
				</table:columns>
			</table:Table>
		</Panel>
	</Dialog>
</core:FragmentDefinition>